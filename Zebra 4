@prefix : <http://www.semanticweb.org/ontologies/zebra#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:House rdf:type owl:Class .
:Nationality rdf:type owl:Class .
:Color rdf:type owl:Class .
:Drink rdf:type owl:Class .
:Cigar rdf:type owl:Class .
:Pet rdf:type owl:Class .

:hasNationality rdf:type owl:ObjectProperty ;
                rdfs:domain :House ;
                rdfs:range :Nationality .
:hasColor rdf:type owl:ObjectProperty ;
          rdfs:domain :House ;
          rdfs:range :Color .
:drinks rdf:type owl:ObjectProperty ;
        rdfs:domain :House ;
        rdfs:range :Drink .
:smokes rdf:type owl:ObjectProperty ;
        rdfs:domain :House ;
        rdfs:range :Cigar .
:keeps rdf:type owl:ObjectProperty ;
       rdfs:domain :House ;
       rdfs:range :Pet .
:rightOf rdf:type owl:ObjectProperty ;
         rdfs:domain :House ;
         rdfs:range :House .
:nextTo rdf:type owl:ObjectProperty ;
        rdfs:domain :House ;
        rdfs:range :House .

:House1 rdf:type :House .
:House2 rdf:type :House .
:House3 rdf:type :House .
:House4 rdf:type :House .
:House5 rdf:type :House .

:Englishman rdf:type :Nationality .
:Spaniard rdf:type :Nationality .
:Ukrainian rdf:type :Nationality .
:Norwegian rdf:type :Nationality .
:Japanese rdf:type :Nationality .

:Red rdf:type :Color .
:Green rdf:type :Color .
:Ivory rdf:type :Color .
:Yellow rdf:type :Color .
:Blue rdf:type :Color .

:Tea rdf:type :Drink .
:Coffee rdf:type :Drink .
:Milk rdf:type :Drink .
:OrangeJuice rdf:type :Drink .
:Water rdf:type :Drink .

:OldGold rdf:type :Cigar .
:Kools rdf:type :Cigar .
:Chesterfields rdf:type :Cigar .
:LuckyStrike rdf:type :Cigar .
:Parliaments rdf:type :Cigar .

:Dog rdf:type :Pet .
:Snails rdf:type :Pet .
:Fox rdf:type :Pet .
:Horse rdf:type :Pet .
:Zebra rdf:type :Pet .

# 1. The Englishman lives in the red house.
[] rdf:type owl:Restriction ;
   owl:onProperty :hasNationality ;
   owl:someValuesFrom :Englishman ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :hasColor ;
     owl:hasValue :Red
   ] .

# 2. The Spaniard owns the dog.
[] rdf:type owl:Restriction ;
   owl:onProperty :hasNationality ;
   owl:someValuesFrom :Spaniard ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :keeps ;
     owl:hasValue :Dog
   ] .

# 3. Coffee is drunk in the green house.
[] rdf:type owl:Restriction ;
   owl:onProperty :drinks ;
   owl:someValuesFrom :Coffee ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :hasColor ;
     owl:hasValue :Green
   ] .

# 4. The Ukrainian drinks tea.
[] rdf:type owl:Restriction ;
   owl:onProperty :hasNationality ;
   owl:someValuesFrom :Ukrainian ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :drinks ;
     owl:hasValue :Tea
   ] .

# 5. The green house is immediately to the right of the ivory house.
:GreenHouse rdf:type owl:Class ;
            owl:equivalentClass [
              rdf:type owl:Restriction ;
              owl:onProperty :hasColor ;
              owl:hasValue :Green
            ] .
:IvoryHouse rdf:type owl:Class ;
            owl:equivalentClass [
              rdf:type owl:Restriction ;
              owl:onProperty :hasColor ;
              owl:hasValue :Ivory
            ] .
:IvoryHouse :rightOf :GreenHouse .

# 6. The Old Gold smoker owns snails.
[] rdf:type owl:Restriction ;
   owl:onProperty :smokes ;
   owl:someValuesFrom :OldGold ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :keeps ;
     owl:hasValue :Snails
   ] .

# 7. Kools are smoked in the yellow house.
[] rdf:type owl:Restriction ;
   owl:onProperty :smokes ;
   owl:someValuesFrom :Kools ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :hasColor ;
     owl:hasValue :Yellow
   ] .

# 8. Milk is drunk in the middle house.
:House3 :drinks :Milk .

# 9. The Norwegian lives in the first house.
:House1 :hasNationality :Norwegian .

# 10. The man who smokes Chesterfields lives in the house next to the man with the fox.
[] rdf:type owl:Restriction ;
   owl:onProperty :smokes ;
   owl:someValuesFrom :Chesterfields ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :nextTo ;
     owl:someValuesFrom [
       rdf:type owl:Restriction ;
       owl:onProperty :keeps ;
       owl:hasValue :Fox
     ]
   ] .

# 11. Kools are smoked in the house next to the house where the horse is kept.
[] rdf:type owl:Restriction ;
   owl:onProperty :smokes ;
   owl:someValuesFrom :Kools ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :nextTo ;
     owl:someValuesFrom [
       rdf:type owl:Restriction ;
       owl:onProperty :keeps ;
       owl:hasValue :Horse
     ]
   ] .

# 12. The Lucky Strike smoker drinks orange juice.
[] rdf:type owl:Restriction ;
   owl:onProperty :smokes ;
   owl:someValuesFrom :LuckyStrike ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :drinks ;
     owl:hasValue :OrangeJuice
   ] .

# 13. The Japanese smokes Parliaments.
[] rdf:type owl:Restriction ;
   owl:onProperty :hasNationality ;
   owl:someValuesFrom :Japanese ;
   owl:onClass [
     rdf:type owl:Restriction ;
     owl:onProperty :smokes ;
     owl:hasValue :Parliaments
   ] .

# 14. The Norwegian lives next to the blue house.
:House1 :hasNationality :Norwegian .
[] rdf:type owl:Restriction ;
   owl:onProperty :nextTo ;
   owl:someValuesFrom [
     rdf:type owl:Restriction ;
     owl:onProperty :hasColor ;
     owl:hasValue :Blue
   ] .

